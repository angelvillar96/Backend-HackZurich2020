[{"id": "40071", "score": 7.272135, "general_info": {"title": "Petersilienwurzelsuppe mit W\u00fcrstchen", "teaser": "Weil Geniessern nicht wurst ist, was sie essen, bereichern herzhafte Schweinschipolata die s\u00e4mige Suppe aus Petersilienwurzeln, Kartoffeln und Halbrahm.", "nutrition": {"calories": 650, "carbohydrates": 26, "carbohydrates_percent": 16.3, "fat": 46, "fat_percent": 64.9, "kilojoule": 2700, "proteins": 30, "proteins_percent": 18.8}, "image_url": "https://recipeimages.migros.ch/medium/bb37a28c495752caf116915a3ece9735a9915aa8/petersilienwurzelsuppe-mit-wuerstchen-0-4-3.jpg"}, "cooking_instructions": {"time": 70, "instructions": "Petersilienwurzel und Kartoffeln sch\u00e4len und in St\u00fccke schneiden. Zwiebel hacken. In der H\u00e4lfte des \u00d6ls d\u00fcnsten. Mit Paprika w\u00fcrzen. Petersilienwurzel- und Kartoffelst\u00fccke beigeben. Kurz mitd\u00fcnsten. Bouillon dazugeben. Bei kleiner Hitze ca. 40 Minuten weich k\u00f6cheln lassen. Mit dem Stabmixer fein p\u00fcrieren. Rahm dazugeben. Mit Salz abschmecken.", "ingredients": {"ingredients": ["Petersilienwurzel", "Petersilie", "Halbrahm", "Oliven\u00f6l", "Schweins-Cipollata", "Zwiebel", "Kartoffel", "Salz", "scharfer Paprika", "Gem\u00fcsebouillon"], "sizes": {"2 Personen": {"Petersilienwurzel": "300 g", "Kartoffeln": "100 g", "Zwiebel": "\u00bd", "Oliven\u00f6l": "1 EL", "scharfes Paprika": "\u00bd TL", "Gem\u00fcsebouillon": "0,6 l", "Halbrahm": "1,25 dl", "Salz": "", "Schweins-Cipolata": "4", "Petersilie": "\u00bc Bund"}, "4 Personen": {"Petersilienwurzel": "600 g", "Kartoffeln": "200 g", "Zwiebel": "1", "Oliven\u00f6l": "2 EL", "scharfes Paprika": "1 TL", "Gem\u00fcsebouillon": "1,2 l", "Halbrahm": "2,5 dl", "Salz": "", "Schweins-Cipolata": "8", "Petersilie": "\u00bd Bund"}, "8 Personen": {"Petersilienwurzel": "1,2 kg", "Kartoffeln": "400 g", "Zwiebeln": "2", "Oliven\u00f6l": "4 EL", "scharfes Paprika": "2 TL", "Gem\u00fcsebouillon": "2,4 l", "Halbrahm": "5 dl", "Salz": "", "Schweins-Cipo