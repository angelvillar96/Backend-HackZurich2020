[{"id": "39914", "score": 2.1624725, "general_info": {"title": "Apfelstrudel mit S\u00fcssmostcreme", "teaser": "Dessert als Hommage an die paradiesischen Fr\u00fcchte: Apfelstrudel mit S\u00fcssmostcreme ehrt den Apfel als Multitalent \u2013 geraffelt im Geb\u00e4ck, gepresst im Most.", "nutrition": {"calories": 430, "carbohydrates": 55, "carbohydrates_percent": 53, "fat": 19, "fat_percent": 41.2, "kilojoule": 1800, "proteins": 6, "proteins_percent": 5.8}, "image_url": "https://recipeimages.migros.ch/medium/3430be5962839a9f91447f3ad7a6e3ea1853ac14/apfelstrudel-mit-suessmostcreme-0-4-3.jpg"}, "cooking_instructions": {"time": 85, "instructions": "F\u00fcr die Creme S\u00fcssmost, Ei und Maisst\u00e4rke in einer Pfanne verr\u00fchren. Unter st\u00e4ndigem R\u00fchren aufkochen. Ca. 2 Minuten unter R\u00fchren k\u00f6cheln lassen, bis die Creme bindet. Pfanne vom Herd nehmen. Kurz weiterr\u00fchren. Creme durch ein Sieb in eine Sch\u00fcssel streichen. Creme direkt mit Klarsichtfolie bedecken und ausk\u00fchlen lassen.", "ingredients": {"ingredients": ["S\u00fcssmost", "s\u00fcsslicher Apfel", "Strudelteigblatt", "Butter", "Maisst\u00e4rke", "Ei", "Zimt", "Haselnuss", "Zucker"], "sizes": {"4 Personen": {"Haseln\u00fcsse": "50 g", "Butter": "40 g", "s\u00fcssliche \u00c4pfel": "4", "Zucker": "2 EL", "Zimt": "\u00bd TL", "Strudelteigbl\u00e4tter \u00e0 30 g": "4 Bl\u00e4tter"}}}}, "allergens": ["N\u00fcsse und Kerne", "Milchprodukte", "Fr\u00fcchte", "Weizen", "Eier"], "nutrition_type": ["Vegetarisch"], "tags": ["Vegetarisch", "Ge\u00fcbt", "Schweiz", "Sommer", "Fr\u00fchling", "Herbst", "Winter"]}, {"id": "39459", "score": 2.1624725, "general_info": {"title": "Bananen-Cr\u00e8me-br\u00fbl\u00e9e mit Tonkabohne", "teaser": "Mit Tonkabohne, die ein bisschen wie Vanille schmeckt, wird die Bananen-Cr\u00e8me-br\u00fbl\u00e9e gew\u00fcrzt. Tonkabohne ist f\u00fcr Kinder unter 6 Jahren nicht geeignet.", "nutrition": {"calories": 460, "carbohydrates": 35, "carbohydrates_percen